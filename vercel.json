{
  "headers": [
    {
      "source": "/(.*)",
      "headers" : [
        {
          "key" : "X-Content-Type-Options",
          "value" : "nosniff"
        },
        {
          "key" : "X-Frame-Options",
          "value" : "DENY"
        },
        {
          "key" : "X-XSS-Protection",
          "value" : "1; mode=block"
        },
        {
          "key" : "Referrer-Policy",
          "value" : "strict-origin"
        },
        {
          "key" : "Content-Security-Policy",
          "value" : "default-src 'self'; img-src *; script-src 'self' nuzlocke-analytics.vercel.app nuzlocke.vercel.app;"
        }
      ]
    }
  ]
}
